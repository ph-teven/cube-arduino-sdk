{
  "name": "CubeArduinoSDK",
  "description": "Useful components for writing arduino applications",
  "authors": [
    {
      "name": "Stefan Beier",
      "maintainer": true
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ph-teven/cube-arduino-sdk"
  },
  "version": "1.3.0",
  "frameworks": "arduino",
  "platforms": "atmelavr, atmelsam, freescalekinetis, nordicnrf51, nxplpc, ststm32, teensy, espressif8266, espressif32, nordicnrf52",
  "examples": [
    {
      "name": "Default",
      "base": "examples",
      "files": [
        "src/main.cpp"
      ]
    }
  ],
  "export": {
    "exclude": [
      "test",
      ".idea",
      ".gitignore",
      "src/main.cpp",
      "cmake-build-debug"
    ]
  },
  "headers": "CubeArduino.h"
}
